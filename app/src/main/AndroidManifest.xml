<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.project.mad.fincheck">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS"/>
    <uses-permission android:name="android.permission.READ_PROFILE"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/x"
        android:label="@string/app_name"
        android:roundIcon="@drawable/x"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity">
        </activity>
        <activity android:name=".InsertFragment">
        </activity>
        <activity android:name=".DayFragment">
        </activity>
        <activity android:name=".IndWeakFragment">
        </activity>
        <activity android:name=".PieCharty">
        </activity>
        <activity android:name=".BarCharty">
        </activity>
        <activity android:name=".RepCharty">
        </activity>
    </application>

</manifest>